{
	"name": "ant-vel/app",
	"description": "Laravel e-commerce Application.",
	"keywords": ["ecommerce", "laravel", "virtual store", "store", "php"],
	"minimum-stability": "dev",
	"homepage": "http://antvel.com",
	"license": "MIT",
	"type": "project",
	"authors": [
		{
			"name": "Gustavo Ocanto",
			"email": "gustavoocanto@gmail.com"
		}
	],
	"support": {
		"issues": "https://github.com/ant-vel/App/issues",
		"source": "https://github.com/ant-vel/App"
	},
	"require": {
		"php": ">=7.1.3",
        "laravel/framework": "5.6.*",
		"laravelcollective/html": "5.6.*",
		"intervention/image": "2.4.*",
		"greggilbert/recaptcha": "2.*",
		"barryvdh/laravel-dompdf": "0.*",
		"barryvdh/laravel-debugbar": "3.*",
		"predis/predis": "^1.0",
		"davejamesmiller/laravel-breadcrumbs": "~5.0",
		"milon/barcode": "5.*",
		"antvel/shop": "^1.3.1",
		"laravel/tinker": "~1.0",
		"laravel/dusk": "~3.0",
		"fideloper/proxy": "~4.0"
	},
	"require-dev": {
		"filp/whoops": "~2.0",
		"fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
		"phpunit/phpunit": "~7.0",
		"nunomaduro/collision": "~2.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-root-package-install": [
			"php -r \"copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"prefer-stable": true
}
